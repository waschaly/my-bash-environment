alias !='history'
alias back='cd $OLDPWD'
alias from='echo $OLDPWD'
alias l='ls -lF $*'
alias li='ls -li $*'
alias ll='ls -alF  $* | $PAGER'
alias llt='ls -alt $* | $PAGER'
alias lltr='ls -altr $* | $PAGER'
alias llS='ls -alS $* | $PAGER'
alias lt='ls -lt $* '
alias ltr='ls -ltr $* '
alias ls='ls --color=auto'
alias lS='ls -lS $* '
alias makelog='$MAKE $* 2>&1 | tee errlog'
alias pl='ps -efww | $PAGER'
